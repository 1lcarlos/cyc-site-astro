---
import '../styles/Layout.css'
import Header from "../components/header.astro";
import Acerca from "../components/acerca.astro";
import Separador from "../components/separador.astro";
import Servicios from "../components/servicios.astro";
import Contacto from "../components/contacto.astro";


---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>CyC Servicios Catastrales</title>
  </head>
  <body>
    <Header />
    <Separador />
    <Acerca/>
    <Separador/>
    <Servicios/>
    <Separador/>
    <Contacto/>

  </body>
</html>
<style>
  *,
  *::before,
  *::after {
    background-color:#050125;
  }
</style>
